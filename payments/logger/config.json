{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "general": {
      "format": "[%(levelname)s]: (%(asctime)s) '%(message)s' at %(module)s.%(funcName)s",
      "datefmt": "%Y-%m-%d %H:%M:%S %z"
    }
  },
  "handlers": {
    "debug": {
      "class": "logging.StreamHandler",
      "formatter": "general",
      "level": "DEBUG",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "payments": {
      "level": "DEBUG",
      "handlers": ["debug"]
    }
  }
}
